{
  "name": "mbt.guide-backend",
  "version": "1.0.0",
  "description": "A user system leveraging rest-hapi to bootstrap your app.",
  "author": "Justin Headley <headley.justin@gmail.com>",
  "repository": "https://github.com/JKHeadley/mbt.guide-backend",
  "license": "MIT",
  "scripts": {
    "start": "npm run start:local",
    "start:local": "NODE_ENV=local nodemon index.js",
    "start:dev": "NODE_ENV=development nodemon index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "test": "jest --coverage",
    "start-docker": "export API_COMMAND='npm run start' && bash run_server.sh",
    "seed": "node scripts/seed.js",
    "seed-docker": "export TASK_COMMAND='npm run seed' && bash run_server.sh",
    "update-permissions": "node scripts/update-permissions.js",
    "update-permissions-docker": "export TASK_COMMAND='npm run update-permissions' && bash run_server.sh",
    "add-captions": "node scripts/add-captions.js",
    "add-captions-docker": "export TASK_COMMAND='npm run add-captions' && bash run_server.sh",
    "recreate-index": "node scripts/es-recreate-index.js",
    "recreate-index-docker": "export API_COMMAND='npm run start' && export TASK_COMMAND='npm run recreate-index' && bash run_server.sh",
    "reindex-segments": "node scripts/es-reindex-segments.js",
    "reindex-segments-docker": "export API_COMMAND='npm run start' && export TASK_COMMAND='npm run reindex-segments' && bash run_server.sh",
    "precommit": "lint-staged",
    "lint": "eslint --fix **/*.js ./"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=12.14.1",
    "npm": ">=3.8.1"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@elastic/elasticsearch": "^8.4.0",
    "@faker-js/faker": "^7.6.0",
    "@hapi/boom": "^10.0.0",
    "@hapi/glue": "^8.0.0",
    "@hapi/hoek": "^10.0.1",
    "@hapi/nes": "^13.0.0",
    "@sentry/node": "^7.17.2",
    "aws-sdk": "^2.1243.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "confidence": "^5.0.1",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "firebase": "^9.13.0",
    "firebase-admin": "^11.2.0",
    "handlebars": "^4.7.7",
    "hapi-auth-jwt2": "^10.2.0",
    "iplocation": "^7.2.0",
    "joi": "^17.6.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "4.17.21",
    "lodash-pickdeep": "^1.0.2",
    "mongoose": "^6.7.0",
    "node-uuid": "^1.4.7",
    "nodemailer": "^6.8.0",
    "nodemailer-markdown": "^1.0.2",
    "q": "^1.5.1",
    "rest-hapi": "^3.0.1",
    "string-similarity": "^4.0.4",
    "useragent": "^2.3.0",
    "youtube-captions-scraper": "^2.0.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "axios": "^1.1.3",
    "csv-stringify": "^6.2.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rewire": "^6.0.0"
  }
}
